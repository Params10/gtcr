{
  "name": "gtcr",
  "description": "A UI for the Kleros powered Generalized Token Curated List",
  "version": "0.1.0",
  "license": "MIT",
  "dependencies": {
    "@loadable/component": "^5.10.1",
    "antd": "^3.20.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.2",
    "redux-starter-kit": "^0.5.1",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:theme": "lessc --js ./src/bootstrap/theme.less ./src/bootstrap/theme.css",
    "test": "react-scripts test",
    "prettify": "kathari prettify",
    "lint": "kathari lint:js --no-root && kathari lint:styled --no-root",
    "lint:fix": "kathari lint:js --no-root --fix && kathari lint:styled --no-root --fix",
    "precommit": "kathari precommit",
    "commitmsg": "kathari commitmsg",
    "cz": "kathari cz",
    "release": "standard-version"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "kathari precommit",
      "commit-msg": "kathari commitmsg"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@kleros/kathari": "^0.23.0",
    "husky": "^3.0.0",
    "less": "^3.9.0",
    "standard-version": "^6.0.1"
  }
}
